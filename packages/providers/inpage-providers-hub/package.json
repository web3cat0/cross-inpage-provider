{
  "name": "@onekeyfe/inpage-providers-hub",
  "version": "1.1.57",
  "keywords": [
    "cross-inpage-provider"
  ],
  "author": "dev-fe@onekey.so",
  "repository": "https://github.com/OneKeyHQ/cross-inpage-provider",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "dist/*"
  ],
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/cjs/index.js"
  },
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "main": "./dist/cjs/index.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc && tsc --project tsconfig.cjs.json",
    "test": "npx playwright test",
    "test:report": "npx playwright show-report reports",
    "test:build": "tsc -p tsconfig.pw.json ",
    "test:run": "npx playwright test -c tests-out",
    "test:try": "node --import=./src/connectButtonHack/universal/__tests__/loader/register.mjs node_modules/.bin/playwright test -c tests-out",
    "start": "tsc --watch"
  },
  "dependencies": {
    "@onekeyfe/cross-inpage-provider-core": "1.1.57",
    "@onekeyfe/cross-inpage-provider-types": "1.1.57",
    "@onekeyfe/onekey-aptos-provider": "1.1.57",
    "@onekeyfe/onekey-btc-provider": "1.1.57",
    "@onekeyfe/onekey-cardano-provider": "1.1.57",
    "@onekeyfe/onekey-conflux-provider": "1.1.57",
    "@onekeyfe/onekey-cosmos-provider": "1.1.57",
    "@onekeyfe/onekey-eth-provider": "1.1.57",
    "@onekeyfe/onekey-nostr-provider": "1.1.57",
    "@onekeyfe/onekey-polkadot-provider": "1.1.57",
    "@onekeyfe/onekey-private-provider": "1.1.57",
    "@onekeyfe/onekey-solana-provider": "1.1.57",
    "@onekeyfe/onekey-starcoin-provider": "1.1.57",
    "@onekeyfe/onekey-sui-provider": "1.1.57",
    "@onekeyfe/onekey-tron-provider": "1.1.57",
    "@onekeyfe/onekey-webln-provider": "1.1.57",
    "web3": "^1.7.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.43.1",
    "playwright": "^1.43.1",
    "@types/node": "^20.12.7"
  }
}